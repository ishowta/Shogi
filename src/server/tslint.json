{
    "extends": [
        "tslint:all",
        "../tslint.json"
    ],
    "rules": {
        "typedef": [
            true,
            // "call-signature",
            // "arrow-call-signature",
            "paramater",
            // "arrow-parameter,
            "property-declaration",
            // "variable-declaration",
            "variable-declaration-ignore-function",
            // "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ],
        "prefer-function-over-method": false,
        "member-access": false
    }
}
